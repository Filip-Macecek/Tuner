{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "bundler",
    // "outDir": "./dist",
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": false,
    "strict": false,
    "allowJs": true,
    "checkJs": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "removeComments": true,
    "noEmit": true
  },
  "include": ["src"],
  "exclude": []
}